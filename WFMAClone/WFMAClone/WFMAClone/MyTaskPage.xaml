<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="WFMAClone.MyTaskPage">
    <ContentPage.Content>
        <StackLayout x:Name="currentTask" Orientation="Vertical">

            <StackLayout Orientation="Horizontal" BackgroundColor="Azure">
                <Label Text="{Binding Job.Id}" FontSize="Small"></Label>
            </StackLayout>


        </StackLayout>
    </ContentPage.Content>
</ContentPage>